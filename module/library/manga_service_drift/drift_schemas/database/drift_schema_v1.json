          "dialect": "sqlite",
          "sql": "CREATE TABLE IF NOT EXISTS \"chapter_tables\" (\"created_at\" INTEGER NOT NULL, \"updated_at\" INTEGER NOT NULL, \"id\" TEXT NOT NULL, \"manga_id\" TEXT NULL, \"title\" TEXT NULL, \"volume\" TEXT NULL, \"chapter\" TEXT NULL, \"translated_language\" TEXT NULL, \"scanlation_group\" TEXT NULL, \"webUrl\" TEXT NULL, \"readable_at\" INTEGER NULL, \"publish_at\" INTEGER NULL, \"last_read_at\" INTEGER NULL, PRIMARY KEY (\"id\"), UNIQUE (\"manga_id\", \"webUrl\", \"title\"), UNIQUE (\"manga_id\", \"title\"), UNIQUE (\"manga_id\", \"webUrl\"), UNIQUE (\"webUrl\", \"title\"), UNIQUE (\"webUrl\"));"
        }
      ]
